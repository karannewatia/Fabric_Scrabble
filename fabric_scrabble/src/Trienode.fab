
public class TrieNode[principal Game, principal allPlayers] {
     public TrieNode[Game, allPlayers]{Game<-} children[] ;

    // isEndOfWord is true if the node represents
    // end of a word
    public boolean isEndOfWord;

    public TrieNode{Game<-} () {
        this.isEndOfWord = false;
        this.children =  new TrieNode[Game, allPlayers][26];

    }
}

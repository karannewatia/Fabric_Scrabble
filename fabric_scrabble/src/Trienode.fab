
public class TrieNode[principal Game] {
     public TrieNode[Game]{Game<-} []{Game<-} children;

    // isEndOfWord is true if the node represents
    // end of a word
    public boolean{Game<-} isEndOfWord;

    public TrieNode{Game<-} () {
        this.isEndOfWord = false;
        this.children =  new TrieNode[Game][26];

    }
}

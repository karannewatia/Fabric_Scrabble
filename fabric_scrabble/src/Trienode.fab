
public class TrieNode[principal User] {
     public TrieNode[User]{User<-} []{User<-} children;

    // isEndOfWord is true if the node represents
    // end of a word
    public boolean{User<-} isEndOfWord;

    public TrieNode{User<-} () {
        this.isEndOfWord = false;
        this.children =  new TrieNode[User][26];

    }
}
